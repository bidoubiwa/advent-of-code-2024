{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "standard-with-typescript",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
         "@typescript-eslint/require-array-sort-compare": "off",
         "no-explicit-any": "off",
          "@typescript-eslint/no-explicit-any": "off"
    }
}
